{
    "presets": ["module:metro-react-native-babel-preset"],
    "env": {
        "production": {
            "plugins": ["react-native-paper/babel"]
        }
    },
    "plugins": [
        [
            "module-resolver",
            {
                "extensions": [".js", ".jsx", ".json"],
                "root": ["."],
                "alias": {
                    "src": "./src",
                    "assets": "./assets",
                    "appConstants": "./appConstants"
                }
            }
        ],
        "react-native-reanimated/plugin"
    ]
}
